<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav.xml"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.seaselmobile.presentation.login.LoginFragment"
        android:label="LoginFragment" />
    <fragment
        android:id="@+id/baseFragment"
        android:name="com.example.seaselmobile.presentation.base.BaseFragment"
        android:label="BaseFragment" >
        <action
            android:id="@+id/action_baseFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
    </fragment>
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.example.seaselmobile.presentation.feedback.FeedbackFragment"
        android:label="FeedbackFragment" />
</navigation>